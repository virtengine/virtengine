{
  "name": "@virtengine/chain-sdk",
  "version": "1.0.0-alpha.20",
  "description": "VirtEngine API TypeScript client",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/virtengine/virtengine.git",
    "directory": "ts"
  },
  "license": "Apache-2.0",
  "author": "VirtEngine Team",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./private-types/*": {
      "import": "./dist/esm/generated/protos/index.*.js",
      "require": "./dist/cjs/generated/protos/index.*.cjs",
      "types": "./dist/types/generated/protos/index.*.d.ts"
    },
    "./web": {
      "import": "./dist/esm/index.web.js",
      "require": "./dist/cjs/index.web.cjs",
      "types": "./dist/types/index.web.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run compile:validators && node ./script/clean-dist.mjs && tsc -p tsconfig.build.json && node esbuild.config.mjs && node --experimental-strip-types --no-warnings ./script/validate-package-exports.ts",
    "compile:validators": "node --experimental-strip-types --no-warnings ./script/compile-json-schema-to-ts.ts",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "release": "npm run build && npm test && npm publish --tag alpha",
    "test": "jest --selectProjects unit functional",
    "test:cov": "jest --selectProjects unit functional --coverage",
    "test:functional": "jest --selectProjects functional",
    "test:unit": "jest --selectProjects unit"
  },
  "lint-staged": {
    "*.json": [
      "sort-json"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "./package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@connectrpc/connect": "^2.0.1",
    "@connectrpc/connect-node": "^2.0.1",
    "@cosmjs/amino": "~0.36.1",
    "@cosmjs/math": "~0.36.1",
    "@cosmjs/proto-signing": "~0.36.1",
    "@cosmjs/stargate": "~0.36.1",
    "base64-js": "^1.5.1",
    "cockatiel": "^3.2.1",
    "js-yaml": "^4.1.1",
    "json-stable-stringify": "^1.3.0",
    "jsrsasign": "^11.1.0",
    "long": "^5.3.2"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^2.2.3",
    "@bufbuild/protoplugin": "^2.2.3",
    "@eslint/js": "^9.21.0",
    "@faker-js/faker": "^9.7.0",
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^4.0.1",
    "@types/dot-object": "^2.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/jsrsasign": "^10.5.15",
    "ajv": "^8.17.1",
    "dot-object": "^2.1.5",
    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "immutability-helper": "^3.1.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "lint-staged": "^15.4.3",
    "lodash": "^4.17.23",
    "sort-json": "^2.0.1",
    "sort-package-json": "^3.0.0",
    "ts-jest": "^29.2.6",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.7",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.29.1"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "volta": {
    "node": "24.12.0",
    "npm": "11.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
